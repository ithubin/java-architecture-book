body {
    margin: 0;
    text-align: center;
    color: #000;
    font: normal 15px Arial, Verdana, Tahoma;
    text-align: center;
    background: #E6CAFF;
    line-height: 150%;
    /*overflow: hidden;*/
}

/* 验证与业务错误信息区域
==========================================================*/
#validator_error {
    background: transparent;
    margin: 1em;
    width: 98%;
    height: 120px;
    border: 1px solid #696;
    padding: 5px 0;
    text-align: left;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 8px;
    -webkit-box-shadow: #666 0px 0px 10px;
    -moz-box-shadow: #666 0px 0px 10px;
    box-shadow: #666 0px 0px 10px;
    background: #FFF5EE;
}

#validator_error p {
    color: red;
}

.validator_error_content {
    padding: 1px 1px 1px 10px;
    height: 120px;
    overflow-y: auto;
    overflow-x: hidden;
}

.datalist {
    border: 1px solid #0058a3; /* table border */
    font-family: Arial;
    border-collapse: collapse;
    background-color: #eaf5ff; /* table background */
    font-size: 14px;
    margin-bottom: 17px;
}

.datalist th {
    border: 1px solid #0058a3; /* line title border */
    background-color: #4bacff; /* line title background */
    color: #FFFFFF; /* line title color */
    font-weight: bold;
    padding-top: 4px;
    padding-bottom: 4px;
    padding-left: 10p;
    padding-right: 10px;
    text-align: center;
}

.datalist td {
    border: 1px solid #0058a3;
    padding-top: 4px;
    padding-bottom: 4px;
    padding-left: 10px;
    padding-right: 10px;
}

.datalist tr:hover,.datalist tr.altrow {
    /* 鼠标经过时行颜色动态变化 */
    background-color: #c4e4ff;}

.tableSearch {
    border: 1px solid #0058a3; /* table border */
    font-family: Arial;
    border-collapse: collapse;
    background-color: #f8ffd3;
    font-size: 14px;
}

.tableSearch td {
    border: 1px solid #0058a3; /* cell border */
    text-align: left;
    padding-top: 4px;
    padding-bottom: 4px;
